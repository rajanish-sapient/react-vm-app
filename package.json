{
    "name": "skinhealth",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "nx run-many --target=build --all",
        "dev": "nx run-many --target=dev --all",
        "dev:ps": "nx run-many --target=dev --projects=ui,utils,pca-skin",
        "dev:emd": "nx run-many --target=dev --projects=ui,utils,elta-md",
        "build:ps": "nx run-many --target=build --projects=ui,utils,pca-skin",
        "build:emd": "nx run-many --target=build --projects=ui,utils,elta-md",
        "lint": "nx run-many --target=lint --all",
        "lint:staged": "lint-staged",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "prepare": "is-ci || husky install",
        "commit": "cz"
    },
    "devDependencies": {
        "@nrwl/cli": "14.3.6",
        "@nrwl/eslint-plugin-nx": "^14.3.6",
        "@nrwl/js": "14.3.6",
        "@nrwl/workspace": "14.3.6",
        "@types/is-ci": "^3.0.0",
        "commitizen": "^4.2.4",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "is-ci": "^3.0.1",
        "jsonc-eslint-parser": "^2.1.0",
        "lint-staged": "^13.0.3",
        "minimist-lite": "^2.2.1",
        "nx": "14.3.6",
        "prettier": "latest",
        "typescript": "~4.7.2"
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    },
    "packageManager": "yarn@1.22.10",
    "lint-staged": {
        "*.{js, jsx, json, ts, tsx}, !tailwind.config.js": [
            "yarn lint --fix",
            "yarn format --uncommitted"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "resolutions": {
        "minimist": "npm:minimist-lite@2.2.1"
    },
    "dependencies": {
        "react-router-dom": "^6.3.0"
    }
}

