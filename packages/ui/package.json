{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --entry.index src/index.tsx --entry.icons src/icons/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint src/**/*.ts*",
    "clean": "rm -rf node_modules && rm -rf dist",
    "storybook": "start-storybook -p 6006",
    "svgr": "svgr --icon --title-prop --no-dimensions --typescript --replace-attr-values \"#555859=currentColor\" -d src/icons static/icons",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.2.1",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-table": "^7.7.12",
    "@types/react-test-renderer": "^18.0.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "config": "*",
    "eslint": "^8.18.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "postcss": "^8.4.14",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.1.4",
    "ts-jest": "^28.0.5",
    "tsconfig": "*",
    "tsup": "^6.1.2",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@radix-ui/react-navigation-menu": "^0.1.2",
    "react-table": "^7.8.0"
  }
}
